{
	"username": "Mexcalith",
	"theme": {
		"name": "M-Theme",
		"backgroundColor": "#3f2f3a",
		"windowColor": "#3f2f3a",
		"textColor": "#fafafa",
		"black": "#241c22",
		"red": "#ed3740",
		"green": "#fcb6e1",
		"yellow": "#d640a2",
		"blue": "#d640a2",
		"magenta": "#d640a2",
		"cyan": "#d640a2",
		"white": "#fafafa",
		"gray": "#d640a2"
	},
	"wallpaper": {
		"url": "",
		"easing": "ease-in-out",
		"fadeIn": true,
		"blur": false
	},
	"terminal": {
		"fixedHeight": true,
		"windowGlow": false,
		"windowGlowColor": "#291f3325",
		"textGlow": false,
		"opacity": 0,
		"blur": 16
	},
	"prompt": {
		"ctrlC": true,
		"placeholder": "command...",
		"placeholderColor": "green",
		"userColor": "magenta",
		"atColor": "red",
		"hostColor": "magenta",
		"promptColor": "green",
		"promptSymbol": ":3",
		"caretColor": "magenta",
		"selectionBg": "green",
		"selectionFg": "textColor"
	},
	"fetch": {
		"timeFormat": "HH:mm",
		"dateFormat": "DD/MM/YYYY",
		"titleColor": "green",
		"image": "assets/default-icon.svg",
		"data": [
			"Time: {time}",
			"Date: {date}",
			"{seperator}",
			"OS: {osName} {osVersion}",
			"Theme: {theme}",
			"{seperator}",
			"Browser: {browser} {browserVersion}",
			"Engine: {engineName}",
			"{seperator}"
		]
	},
	"urlLaunch": {
		"target": "_self",
		"hoverColor": "magenta"
	},
	"search": {
		"default": "https://searx.be/search?q=",
		"target": "_self",
		"shortcutRegex": "([A-Za-z0-9]+) (.*)",
		"shortcuts": [
			{
				"alias": "sx",
				"name": "SearX Search",
				"url": "https://searx.be/search?q={}"
			},
			{
				"alias": "d",
				"name": "DuckDuckGo Search",
				"url": "https://duckduckgo.com/?q={}"
			},
			{
				"alias": "b",
				"name": "Brave Search",
				"url": "https://search.brave.com/search?q={}"
			},
			{
				"alias": "gh",
				"name": "Github Search",
				"url": "https://github.com/search?q={}"
			},
			{
				"alias": "s",
				"name": "Stack Overflow Search",
				"url": "https://stackoverflow.com/search?q={}"
			},
			{
				"alias": "r",
				"name": "Subreddit Search",
				"url": "https://reddit.com/r/{}"
			},
			{
				"alias": "w",
				"name": "Wikipedia Search",
				"url": "https://en.wikipedia.org/wiki/{}"
			}
		]
	},
